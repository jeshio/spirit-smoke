Чтобы посмотреть диаграмму нужно вставить код сюда:
https://cruise.eecs.uottawa.ca/umpleonline/umple.php

class product
{
  name;
  slug;
  float priceInRub;
  count;
  imageUrl;
  model;
  description;
  int weightInGrams;
  * -- 1 productCategory;
  1 -- * company;
}

class productCategory
{
  name;
  slug;
  description;
}

class feature
{
  name;
  slug;
  imageUrl;
  boolean invisible;
  * -- * product;
  * -- * productCategory;
}

class discount
{
  name;
  percent;
  rub;
  code;
  startDate;
  endDate;
  boolean isGlobal;
  boolean forAll;
  float minimumSum;
int count;
boolean disabled;
int minProductsCount;
* -- * productCategory;
  * -- * product;
  * -- * bonus;
  * -- * order;
  * -- * feature;
  * -- * delivery;
}

class order
{
  address;
  status;
  intercomCode;
  personsCount;
  comment;
  deliveryTime;
  phoneNumber;
  ourComment;
  boolean isCash;
  float sum;
}

class productOrder
{
  int productsCount;
  1 -- 1 product;
  1 -- 1 order;
}

class bonus
{
  name;
  slug;
  conditionsDescription;
  iconUrl;
* -- * order;
}

class company
{
  name;
  slug;
1 -- * productCategory;
}

class promotion
{
  name;
  description;
  imageUrl;
  startDate;
  endDate;
  int priority;
}

class params
{
  name;
  slug;
  value;
}

class delivery
{
  sum;
  status;
}

class productDelivery
{
  int count;
  float costsInRub;
  1 -- * product;
  1 -- * delivery;
}
